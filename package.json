{
  "name": "husky4th",
  "version": "4.0.1",
  "description": "Prevents bad commit or push (git hooks, pre-commit/precommit, pre-push/prepush, post-merge/postmerge and all that stuff...)",
  "scripts": {
    "test": "husky install && npm run postinstall",
    "postinstall": "node postinstall.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cncolder/husky4th.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "git",
    "hook",
    "hooks",
    "pre-commit",
    "precommit",
    "post-commit",
    "postcommit",
    "pre-push",
    "prepush",
    "post-merge",
    "postmerge",
    "test",
    "lint"
  ],
  "author": "colder",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cncolder/husky4th/issues"
  },
  "homepage": "https://github.com/cncolder/husky4th#readme",
  "dependencies": {
    "husky": "^4.3.8"
  },
  "devDependencies": {
    "husky8": "npm:husky@^8"
  }
}
